    "props": {
      "src": "https://storage.beko.co.uk/assets/beko/landing-page/about/images/sustainability/4@1x.jpg",
      "blockClass": "homeCardBackground"
    }
  },
  "flex-layout.col#home-card-3-about-us": {
    "title": "[COL] - Home Card Oven",
    "props": {
      "blockClass": "homeCard-about-us",
      "verticalAlign": "middle"
    },
    "children": [
      "rich-text#home-card-about-us-3-title",
      "rich-text#home-card-about-us-3-subtitle",
      "link#abous-us-card-2-cta"
    ]
  },
  "rich-text#home-card-about-us-3-title": {
    "title": "[RICH TEXT] - Home Card 1",
    "props": {
      "text": "## Transforming waste material\n For healthy living, on a healthy planet",
      "textAlignment": "center",
      "blockClass": "homeCard-about-us-title"
    }
  },
  "rich-text#home-card-about-us-3-subtitle": {
    "title": "[RICH TEXT] - Home Card 1",
    "props": {
      "text": "## Using innovative technology, we recycle fishing nets and industrial thread waste, transforming them into high-performance materials used in the door décor plastic, display cover, and inner plastic display cover of our RecycledNet® ovens. As a company, we have set a target for there to be 40% recycled plastic in all products by 2030. This target will be reached without compromising the quality or reliability of our appliances.",
      "textAlignment": "center",
      "blockClass": "homeCard-about-us"
    }
  },
  "link#abous-us-card-2-cta": {
