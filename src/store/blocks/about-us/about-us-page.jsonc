  "image#home-card-1-background-about-us": {
    "title": "[IMAGE] - Card 1 Background",
    "props": {
      "src": "https://storage.beko.co.uk/assets/beko/landing-page/about/images/sustainability/1@1x.jpg",
      "blockClass": "homeCardBackground"
    }
  },
  "flex-layout.col#home-card-1-about-us": {
    "title": "[COL] - Home Card Oven",
    "props": {
      "blockClass": "homeCard-about-us",
      "verticalAlign": "middle"
    },
    "children": [
      "rich-text#home-card-about-us-1-title",
      "rich-text#home-card-about-us-1-subtitle"
    ]
  },
  "rich-text#home-card-about-us-1-title": {
    "title": "[RICH TEXT] - Home Card 1",
    "props": {
      "text": "## Sustainability \nChange Starts Today",
      "textAlignment": "center",
      "blockClass": "homeCard-about-us-title"
    }
  },
  "rich-text#home-card-about-us-1-subtitle": {
    "title": "[RICH TEXT] - Home Card 1",
    "props": {
      "text": "## We strive to minimise the impact we have on the environment. With the biggest part of our carbon emissions resulting from the use of our products by our customers, we not only need to look at how we produce our products and the materials we use, but we also need to ensure we are bringing energy-efficient products to the market. As a company, we are committed to improving the average energy efficiency of our products year after year.",
      "textAlignment": "center",
      "blockClass": "homeCard-about-us"
    }
  },
  "flex-layout.row#home-card-about-us-2": {
    "title": "[ROW] - About us Card 2",
    "props": {
      "blockClass": "homeCard",
      "fullWidth": true
    },
    "children": [
      "image#home-card-2-background-about-us",
      "flex-layout.col#home-card-2-about-us"
    ]
  },
  "image#home-card-2-background-about-us": {
    "title": "[IMAGE] - Card 1 Background",
    "props": {
      "src": "https://storage.beko.co.uk/assets/beko/landing-page/about/images/sustainability/2@1x.jpg",
      "blockClass": "homeCardBackground"
    }
  },
  "flex-layout.col#home-card-2-about-us": {
    "title": "[COL] - Home Card Oven",
    "props": {
      "blockClass": "homeCard-about-us",
      "verticalAlign": "middle",
      "textAlignment": "center"
    },
    "children": [
      "rich-text#home-card-about-us-2-title",
      "rich-text#home-card-about-us-2-subtitle",
      "link#abous-us-card-1-cta"
    ]
  },
  "link#abous-us-card-1-cta": {
    "title": "[LINK] - Home Card 1 CTA",
    "props": {
      "label": "Discover RecycledTub range",
      "href": "#",
      "blockClass": [
        "homeCardCta",
        "cardCtaGreenAboutUs"
      ]
    }
  },
  "rich-text#home-card-about-us-2-title": {
    "title": "[RICH TEXT] - Home Card 1",
    "props": {
      "text": "## Recycling \nA new lease of life for plastic",
      "textAlignment": "center",
      "blockClass": "homeCard-about-us-title"
    }
  },
  "rich-text#home-card-about-us-2-subtitle": {
    "title": "[RICH TEXT] - Home Card 1",
    "props": {
      "text": "## We are also working to increase the use of recycled materials within our appliances. Our washing machines, tumble dryers, cookers, and dishwashers now feature parts made from recycled materials, including plastic bottles and discarded fishing nets. By transforming waste materials for use in our appliances, we are taking small steps to reduce the embedded carbon footprint of our products, whilst also promoting the circularity of materials.",
      "textAlignment": "center",
      "blockClass": "homeCard-about-us"
    }
  },
  "flex-layout.row#home-card-about-us-3": {
    "title": "[ROW] - Home Card 1",
    "props": {
      "blockClass": "homeCard",
      "fullWidth": true
    },
    "children": [
